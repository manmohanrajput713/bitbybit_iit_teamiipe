

<%  if(error && error.length){  %>
    <div class="alert">
        <%= error %> 
        <button class="close-btn" onclick="this.parentElement.style.display='none'">&times;</button>
    </div>
    <%  }  %>
    
    <%  if(success && success.length){  %>
        <div class="alert-success">
            <%= success %> 
            <button class="close-btn" onclick="this.parentElement.style.display='none'">&times;</button>
        </div>
        <%  }  %>